<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <Nullable>enable</Nullable>
        <PackageId>HttpRequestToCurl</PackageId>
        <Authors>James Dorevski</Authors>
        <Description>Convert HttpRequestMessage objects into cURL commands</Description>
        <PackageProjectUrl>https://github.com/jamesdorevski/httprequest-to-curl</PackageProjectUrl>
        <PackageTags>curl;http;requests</PackageTags>
        <PackageLicenseExpression>GPL-3.0-or-later</PackageLicenseExpression>
    </PropertyGroup>
        
    <ItemGroup>
        <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleTo">
            <_Parameter1>HttpRequestToCurl.Tests</_Parameter1>
        </AssemblyAttribute>
    </ItemGroup>

</Project>
